{
  "name": "apollo",
  "version": "10.1.0",
  "license": "PrimeNG Commercial",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=local",
    "build": "ng build",
    "build:local": "ng build --configuration=local --output-path dist/local && echo 'local build ready!'",
    "build:dev": "ng build --configuration=dev --aot --output-path dist/dev && echo 'dev build ready!'",
    "build:stage": "ng build --configuration production --configuration=stage --stats-json=false --output-path dist/stage && echo 'stage build ready!'",
    "build:prod": "ng build --configuration production --configuration=prod --aot --output-path dist/prod && echo 'prod build ready!'",
    "bundle-report": "webpack-bundle-analyzer _builds/dist/prod/stats.json",
    "open:local": "npm run build:local && cd dist/local && http-server -o",
    "open:dev": "npm run build:dev && cd dist/dev && http-server -o",
    "open:stage": "npm run build:stage && cd dist/stage && http-server -o",
    "open:beta": "npm run build:beta && cd dist/beta && http-server -o",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.11",
    "@angular/cdk": "~12.2.11",
    "@angular/common": "~12.2.11",
    "@angular/compiler": "~12.2.11",
    "@angular/core": "^12.2.11",
    "@angular/forms": "~12.2.11",
    "@angular/platform-browser": "~12.2.11",
    "@angular/platform-browser-dynamic": "~12.2.11",
    "@angular/router": "~12.2.11",
    "@fullcalendar/core": "4.0.2",
    "@fullcalendar/daygrid": "4.0.1",
    "@fullcalendar/interaction": "4.0.2",
    "@fullcalendar/timegrid": "4.0.1",
    "@ngneat/error-tailor": "^2.1.0",
    "@rxweb/reactive-form-validators": "^2.1.3",
    "@types/intro.js": "^3.0.0",
    "file-saver": "^2.0.2",
    "font-awesome": "4.7.0",
    "intl": "1.2.5",
    "intro.js": "^3.0.1",
    "ng2-cookies": "^1.0.12",
    "ngx-clipboard": "^13.0.1",
    "powerbi-client-angular": "^1.0.1",
    "powerbi-report-component": "^2.6.0",
    "primeflex": "2.0.0",
    "primeicons": "4.0.0",
    "primeng": "12.2.1",
    "prismjs": "^1.25.0",
    "quill": "^1.3.7",
    "rxjs": "~6.5.5",
    "terser": "^5.7.1",
    "tslib": "^2.0.0",
    "ua-parser-js": "^0.7.30",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.11",
    "@angular/cli": "~12.2.11",
    "@angular/compiler-cli": "~12.2.11",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.5",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}
